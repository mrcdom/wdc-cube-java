<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='shopping'>
	<inherits name="com.google.gwt.core.Core" />
	<inherits name="com.google.gwt.resources.Resources" />
	<inherits name="com.google.gwt.storage.Storage" />
	<inherits name="elemental2.core.Core" />
	<inherits name="elemental2.dom.Dom" />
	<inherits name="elemental2.promise.Promise" />
	<inherits name="ru.finam.slf4jgwt.logging.gwt.Logging"/>
	<inherits name="ru.finam.slf4jgwt.api.API"/>

	<inherits name="br.com.wedocode.framework.WeDoCodeCommons" />
	<inherits name="br.com.wedocode.framework.WeDoCodeWebflow" />

	<source path="presentation" />
	<source path="view" />
	
	<!-- Specify the app entry point class. -->
	<entry-point class="br.com.wedocode.shopping.view.gwt.Main" />
	
	<!-- allow Super Dev Mode -->
	<add-linker name="xsiframe"/>
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
	
	<set-property name="gwt.logging.logLevel" value="INFO"/>
</module>